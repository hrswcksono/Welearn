<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_bg"
    tools:context=".presentation.ui.huruf.multiplayer.canvas.HurufLevelTigaActivity">

  <TextView
      android:id="@+id/tv_selesai_h3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="@string/menunggu_selesai"
      android:textColor="@color/black"
      android:textSize="20sp"
      app:layout_constraintBottom_toTopOf="@+id/cnvsLevelTigaHuruftwo"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view4" />

  <ImageView
      android:id="@+id/levelTigaHurufBack"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      android:src="@drawable/back_button"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageView
      android:id="@+id/submit_tiga_huruf"
      android:layout_width="51dp"
      android:layout_height="51dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="20dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view4"
      app:srcCompat="@drawable/submit_btn" />

  <ImageView
      android:id="@+id/btn_user_participant_h3"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginTop="10dp"
      android:layout_marginEnd="20dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/team_btn" />

  <View
      android:id="@+id/view4"
      android:layout_width="370dp"
      android:layout_height="45dp"
      android:layout_marginTop="15dp"
      android:background="@color/main_green"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/level_huruf_ke" />

  <TextView
      android:id="@+id/level_huruf_ke"
      android:layout_width="190dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="20dp"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="@string/level_huruf"
      android:textAlignment="center"
      android:textColor="@color/black"
      android:textSize="30sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/soal_huruf_dipilih"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="15dp"
      android:layout_marginEnd="20dp"
      android:textColor="@color/black"
      android:fontFamily="@font/rubik_regular"
      android:text="@string/soal_dipilih"
      android:textAlignment="textStart"
      android:textSize="15sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="@+id/view4"
      app:layout_constraintEnd_toEndOf="@+id/view4"
      app:layout_constraintStart_toEndOf="@+id/spk_tiga_huruf"
      app:layout_constraintTop_toTopOf="@+id/view4" />

  <ImageView
      android:id="@+id/spk_tiga_huruf"
      android:layout_width="29dp"
      android:layout_height="0dp"
      android:layout_marginStart="4dp"
      app:layout_constraintBottom_toBottomOf="@+id/view4"
      app:layout_constraintStart_toStartOf="@+id/view4"
      app:layout_constraintTop_toTopOf="@+id/view4"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/voice" />

  <ImageView
      android:id="@+id/refresh_tiga_huruf"
      android:layout_width="51dp"
      android:layout_height="51dp"
      android:layout_marginStart="21dp"
      android:layout_marginTop="12dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view4"
      app:srcCompat="@drawable/reset_btn" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufone"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="72dp"
      android:background="@color/white"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view4" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHuruftwo"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="72dp"
      android:background="@color/white"
      app:layout_constraintEnd_toStartOf="@+id/cnvsLevelTigaHurufthree"
      app:layout_constraintStart_toEndOf="@+id/cnvsLevelTigaHurufone"
      app:layout_constraintTop_toBottomOf="@+id/view4" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufthree"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="72dp"
      android:background="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view4" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHuruffour"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="12dp"
      android:background="@color/white"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHurufthree" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHuruffive"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="12dp"
      android:background="@color/white"
      app:layout_constraintEnd_toStartOf="@+id/cnvsLevelTigaHurufsix"
      app:layout_constraintStart_toEndOf="@+id/cnvsLevelTigaHuruffour"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHurufone" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufsix"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="13dp"
      android:background="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHuruftwo" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufseven"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="13dp"
      android:background="@color/white"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHuruffive" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufeight"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="13dp"
      android:background="@color/white"
      app:layout_constraintEnd_toStartOf="@+id/cnvsLevelTigaHurufnine"
      app:layout_constraintStart_toEndOf="@+id/cnvsLevelTigaHurufseven"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHurufsix" />

  <com.divyanshu.draw.widget.DrawView
      android:id="@+id/cnvsLevelTigaHurufnine"
      android:layout_width="130dp"
      android:layout_height="130dp"
      android:layout_marginTop="13dp"
      android:background="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/cnvsLevelTigaHuruffour" />

  <ProgressBar
      android:id="@+id/progressBarH3"
      style="?android:attr/progressBarStyle"
      android:layout_width="100dp"
      android:layout_height="100dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/number_canvas_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="1"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufone"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufone" />

  <TextView
      android:id="@+id/number_canvas_"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="2"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHuruftwo"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHuruftwo" />

  <TextView
      android:id="@+id/number_canvas_4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="4"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHuruffour" />

  <TextView
      android:id="@+id/number_canvas_5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="5"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHuruffive"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHuruffive" />

  <TextView
      android:id="@+id/number_canvas_6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="6"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufsix"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufsix" />

  <TextView
      android:id="@+id/number_canvas_7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="7"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufseven"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufseven" />

  <TextView
      android:id="@+id/number_canvas_8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="8"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufeight"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufeight" />

  <TextView
      android:id="@+id/number_canvas_9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="9"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufnine"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufnine" />

  <TextView
      android:id="@+id/number_canvas_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/rubik_semi_bold"
      android:text="3"
      android:textColor="@color/black"
      android:textSize="22sp"
      app:layout_constraintStart_toStartOf="@+id/cnvsLevelTigaHurufthree"
      app:layout_constraintTop_toTopOf="@+id/cnvsLevelTigaHurufthree" />


</androidx.constraintlayout.widget.ConstraintLayout>